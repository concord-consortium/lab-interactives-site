<component><class>org.concord.modeler.PageApplet</class>
<appletjar>energy2d.jar</appletjar>
<appletclass>org.concord.energy2d.system.System2D</appletclass>
<resource>2$applet$0.aps</resource><parameter>{name=&quot;script&quot; value=&quot;set convective false;
set timestep 10;
set ray_speed .002;
set emission_interval 10;
set background_conductivity 0.001;
part rectangle (9, 7.5, 0.5, 5);
set part[0].conductivity .001;
set part[0].emissivity 1;
set part[0].filled 0;
set part[0].constant_temperature 1;
set part[0].temperature 100;

part polygon (2, 2, 6, 8, 6.5, 7.5, 2.5, 1.5);
set part[1].filled 0;
set part[1].conductivity 0.1;

thermometer (9.1, 5);
thermometer (4, 5);
boundary temperature(0, 0, 0, 0);&quot;}
</parameter>
<width>400</width><height>400</height>
<border>Lowered Etched</border><caching>true</caching></component></content>

