<component><class>org.concord.modeler.PageApplet</class>
<appletjar>energy2d.jar</appletjar>
<appletclass>org.concord.energy2d.system.System2D</appletclass>
<resource>8$applet$0.aps</resource><parameter>{name=&quot;script&quot; value=&quot;set isotherm false; 
set timestep .2;
set convective true;
set thermal_buoyancy 0;
set vorticity_confinement false;
set background_conductivity 1;
set viscosity 0.0001;
set viewupdate 10;
part rectangle(3, 1, 4, 0.5);
set part[0].wind_speed .05;
set part[0].wind_angle -90;
set part[0].temperature 20;
part ellipse(2, 5, 1, 1);
thermometer (2, 5);
thermometer (4, 5);
thermometer (8, 5);
boundary flux(0, 0, 0, 0);&quot;}
</parameter>
<width>400</width><height>400</height>
<border>Lowered Etched</border><caching>true</caching></component></content>

