<tick>true</tick>
<labeltable>{value=&quot;40.0&quot;, label=&quot;High&quot;}{value=&quot;0.0&quot;, label=&quot;Low&quot;}</labeltable>
<script>set heatbath 500+%val*200;</script>
<change>Execute MW script</change>
