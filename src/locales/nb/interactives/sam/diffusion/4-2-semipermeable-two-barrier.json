    },
    {
      "id": "trace-atom",
      "type": "checkbox",
      "text": "Spor et molekyl",
      "textOn": "right",
      "width": "auto",
      "height": "auto",
      "property": "tracingAtom",
      "retainProperty": true,
      "disabled": false,
      "tooltip": ""
    }
  ],
  "layout": {
    "bottom": [
      "barrier-height",
      "trace-atom"
    ],
    "right": []
  },
  "template": "simple"
}
