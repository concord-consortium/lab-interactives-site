      "type": "barGraph",
      "property": "reactionComplete",
      "min": 0,
      "max": 100,
      "title": "Ferdig reagert",
      "titleOn": "right",
      "labels": 10,
      "units": true,
      "gridLines": 10,
      "labelFormat": ".0f",
      "averagePeriod": 10000,
      "barWidth": "2em",
      "height": "100%",
      "barColor": "#e23c34",
      "fillColor": "#fff",
      "tooltip": ""
    },
    {
      "id": "slow-motion",
      "type": "checkbox",
      "text": "Sakte bevegelse",
      "textOn": "right",
      "width": "auto",
      "height": "auto",
      "property": "slowMotion",
      "retainProperty": true,
      "disabled": false,
      "tooltip": ""
    },
    {
      "id": "add-catalyst",
      "type": "button",
      "action": "addRandomAtom(3);",
      "text": "Legg til katalysator",
      "width": "",
      "height": "",
      "disabled": false,
      "tooltip": ""
    }
  ],
  "layout": {
    "right": [
      "reaction-complete"
    ],
