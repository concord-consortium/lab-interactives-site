  "layout": {

     "playback-container": [
      "playback"
    ],
    "bottomLeft": [
      [
        "windSlider"
      ],
      [
        "temperatureSlider"
      ],
       [
        "sunSlider"
      ],
      [
        "rainSlider"
      ]
    ],
    "bottomRight": [
           [
        "carsSlider"
      ],
      [
        "carPollutionSlider"
      ],
      [
        "electricCarsSlider"
      ],
      [
        "factoriesSlider"
      ],
      [
        "factoryPollutionSlider"
      ]
    ],
    "topRight": [
      [
        "aqiGraph"
      ]
    ]

  },
 "template": [
    {
      "id": "playback-container",
      "top": "model.bottom",
      "left": "model.left"
    },
    {
      "id": "bottomLeft",
      "top": "playback-container.bottom",
      "left": "model.left"
    },
    {
      "id": "bottomRight",
      "top": "playback-container.bottom",
      "left": "bottomLeft.right"
    },
    {
      "id": "topRight",
      "top": "model.top",
      "left": "model.right",
      "width": "0.75 * model.width",
      "height": "model.height",
      "padding-left": "0.5em"
    }
  ]
}
