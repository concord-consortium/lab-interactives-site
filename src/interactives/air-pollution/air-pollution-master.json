      "fillToValue": 0,
      "labels": [
        {
          "value": "-100",
          "label": "<i class='icon-arrow-left'></i>"
        },
        {
          "value": 0,
          "label": "None"
        },
        {
          "value": "100",
          "label": "<i class='icon-arrow-right'></i>"
        }
      ],
      "width": "18em",
      "height": "auto",
      "property": "windSpeed",
      "retainProperty": true,
      "disabled": false,
      "tooltip": "Change the speed and direction of the wind.",
      "helpIcon": false
    },
    {
      "id": "carsSlider",
